{
  "manifest_version": 2,
  "name": "peek-jwt",
  "description": "Peek at contents of JSON Web Token (JWT)",
  "author": "Yuki Furuyama",
  "version": "0.1",
  "icons": {
    "16": "icon/icon_16x16.png",
    "48": "icon/icon_48x48.png",
    "128": "icon/icon_128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["lib/tippy.2.0.9.all.min.js", "main.js"],
      "css": ["main.css"]
    }
  ],
  "permissions": []
}
